<AnchorPane fx:id="PaneMain" prefHeight="749.0" prefWidth="1298.0" style="-fx-background-color: white;" stylesheets="@../css/menu_items.css" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controller.HomeFragment">
    <children>
        <Separator layoutY="73.0" prefHeight="0.0" prefWidth="1298.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
        <Text fill="#2b579a" layoutX="79.0" layoutY="60.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Home">
            <font>
                <Font name="Segoe UI Semilight" size="40.0" />
            </font>
        </Text>
        <Button fx:id="createBtn" layoutX="83.0" layoutY="145.0" mnemonicParsing="false" onAction="#createTimeline" styleClass="btn" text="Create timeline" />
        <Button fx:id="loadBtn" layoutX="295.0" layoutY="145.0" mnemonicParsing="false" onAction="#loadTimeline" styleClass="btn" text="Load timeline" />
      <ImageView fitHeight="130.0" fitWidth="147.0" layoutX="103.0" layoutY="208.0" onMouseClicked="#createTimeline" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/sr.png" />
